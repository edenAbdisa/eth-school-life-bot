create Table FireTable( id text primary key ,userId text ,postId text );

//User Media Article Company Event 
create Table User (
             id text primary key,f_name varchar(40) not null,l_name varchar(40) not null, birth_date date not null,gender varchar(1) not null, level_study varchar(50) not null,
             pp_media_id text ,cv_media_id text,email varchar(100) ,phone_number varchar(100),role varchar(100)
              );